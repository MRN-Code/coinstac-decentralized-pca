{
  "meta": {
    "name": "decentralized pca demo",
    "id": "decentralized-pca",
    "version": "v1.0.0",
    "repository": "",
    "description": "a demo for decentralized pca"
  },
  "computation": {
    "type": "docker",
    "dockerImage": "decentralized-pca",
    "command": [
      "python",
      "\/computation\/local.py"
    ],
    "remote": {
      "type": "docker",
      "dockerImage": "decentralized-pca",
      "command": [
        "python",
        "\/computation\/remote.py"
      ]
    },
    "input": {
      "data": {
        "label": "Input Data",
        "type": "array",
        "extensions": [
          [
            "npz"
          ]
        ]
      },
      "row_mean_global": {
        "label": "Global Row Means",
        "type": "array",
        "extensions": [
          [
            "npz"
          ]
        ]
      }
    },
    "output": {
      "PC_global": {
        "label": "Global Principal Components",
        "type": "array",
        "extensions": [
          [
            "npz"
          ]
        ]
      },
      "projM_global": {
        "label": "Global Projection (Whitening) Matrix",
        "type": "array",
        "extensions": [
          [
            "npz"
          ]
        ]
      },
      "bkprojM_global": {
        "label": "Global Backprojection (Dewhitening) Matrix",
        "type": "array",
        "extensions": [
          [
            "npz"
          ]
        ]
      }
    }
  }
}